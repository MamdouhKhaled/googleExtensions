{
	"name": "Mamdouh",
	"description": "My On New Tab",
	"version": "1.0",
	"manifest_version": 2,
	"icons": {
		"128": "icon.png"
	},
	"browser_action": {
		"default_title": "New Tab",
		"default_popup": "popup.html"
	},
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions" : ["activeTab","topSites","bookmarks","http://www.google.com/","https://www.google.com/"]
}
